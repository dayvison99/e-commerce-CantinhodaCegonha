<?xml version="1.0" encoding="UTF-8" ?>
<fonts>
	<font>
		<title>Header Font</title>
		<id>header-font</id>
		<subtitle>Header font subtitle</subtitle>
		<google>1</google>
		<output>header, #header</output>
		<def>
			<color>#5d5d5d</color>
			<size>13px</size>
			<family>Roboto</family>
			<weight>400</weight>
			<align>inherit</align>
			<line_height>13px</line_height>
		</def>
	</font>
	<font>
		<title>Body Font</title>
		<id>body-font</id>
		<subtitle>body font subtitle</subtitle>
		<default>Roboto</default>
		<output>body</output>
		<def>
			<color>#5d5d5d</color>
			<size>13px</size>
			<line_height>1.55em</line_height>
			<family>Roboto</family>
			<weight>400</weight>
			<align>inherit</align>
		</def>
	</font>
	
	<font>
		<title>Footer Font</title>
		<id>footer-font</id>
		<subtitle>footer font subtitle</subtitle>
		<default>Roboto</default>
		<output>footer, #footer</output>
		<def>
			<color>#5d5d5d</color>
			<size>13px</size>
			<family>Roboto</family>
			<weight>400</weight>
			<align>inherit</align>
			<line_height>13px</line_height>
		</def>
	</font>
</fonts>