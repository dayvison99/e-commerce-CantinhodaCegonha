<?xml version="1.0" encoding="UTF-8" ?>
<fonts>
	<font>
		<title>Header Font</title>
		<id>header-font</id>
		<subtitle>Header font subtitle</subtitle>
		<google>1</google>
		<output>header, #header, .pc-menu > .menu > li > a
		</output>
		<def>
			<color>#3d3d3d</color>
			<size>12px</size>
			<family>Poppins</family>
			<weight>400</weight>
			<align>inherit</align>
			<line_height>12px</line_height>
		</def>
	</font>
	<font>
		<title>Shipping Text Header</title>
		<id>shipping-text-header</id>
		<subtitle>shipping text header font subtitle</subtitle>
		<default>Poppins</default>
		<output>#header.header-12 .text-color1</output>
		<def>
			<size>28px</size>
			<family>Poppins</family>
			<weight>700</weight>
			<align>inherit</align>
		</def>
	</font>
	<font>
		<title>Body Font</title>
		<id>body-font</id>
		<subtitle>body font subtitle</subtitle>
		<default>Poppins</default>
		<output>body, .woocommerce .products section.product .product-inner h3.product-title, 
		.woocommerce-page .products section.product .product-inner h3.product-title, entry-month,
		.woocommerce ul.product_list_widget li a, ul.product_list_widget li a</output>
		<def>
			<color>#3d3d3d</color>
			<size>12px</size>
			<line_height>16px</line_height>
			<family>Poppins</family>
			<weight>400</weight>
		</def>
	</font>
	
	<font>
		<title>Post Entry Day Font</title>
		<id>post-entry-day-font</id>
		<subtitle>post entry day subtitle</subtitle>
		<default>Poppins</default>
		<output>.entry-day</output>
		<def>
			<color>#5d5d5d</color>
			<size>30px</size>
			<line_height>30px</line_height>
			<family>Poppins</family>
			<weight>400</weight>
		</def>
	</font>
	
	
	<font>
		<title>Footer Font</title>
		<id>footer-font</id>
		<subtitle>footer font subtitle</subtitle>
		<default>Poppins</default>
		<output>footer, #footer</output>
		<def>
			<color>#8d8d8d</color>
			<size>12px</size>
			<family>Poppins</family>
			<weight>400</weight>
			<align>inherit</align>
			<line_height>13px</line_height>
		</def>
	</font>
	
	<font>
		<title>Footer Heading Font</title>
		<id>footer-heading-font</id>
		<default>Poppins</default>
		<output>#footer .heading-title</output>
		<def>
			<color>#998294</color>
			<size>14px</size>
			<family>Poppins</family>
			<weight>700</weight>
			<text_transform>uppercase</text_transform>
			<align>inherit</align>
			<line_height>16px</line_height>
		</def>
	</font>
	
	
	<font>
		<title>Main menu Font</title>
		<id>main-menu-font</id>
		<subtitle>main menu font subtitle</subtitle>
		<default>Poppins</default>
		<output>.pc-menu > .menu.menu-vertical-menu > li > a</output>
		<def>
			<color>#fff</color>
			<size>15px</size>
			<family>Poppins</family>
			<weight>400</weight>
			<align>inherit</align>
			<line_height>15px</line_height>
		</def>
	</font>
	
	<font>
		<title>Product Price Font</title>
		<id>product-price-font</id>
		<subtitle>product price font subtitle</subtitle>
		<default>Poppins</default>
		<output>span.amount</output>
		<def>
			<color>#3d3d3d</color>
			<size>16px</size>
			<family>Poppins</family>
			<weight>700</weight>
			<align>inherit</align>
			<line_height>16px</line_height>
		</def>
	</font>
	
