	<font>
		<title>Heading Font</title>
		<id>heading-font</id>
		<subtitle>heading font subtitle</subtitle>
		<default>Poppins</default>
		<output>.vc_tta-style-classic.vc_tta-tabs.vc_general.nth-tab-style4 .vc_tta-tab > a, .heading_center .heading-title, .heading-title, .heading-title-f </output>
		<def>
			<size>26px</size>
			<family>Poppins</family>
			<weight>400</weight>
			<line_height>26px</line_height>
			<text_transform>none</text_transform>
		</def>
	</font>
	
	<font>
		<title>Post Title</title>
		<id>post-title-font</id>
		<subtitle>post heading font subtitle</subtitle>
		<default>Poppins</default>
		<output>h3 .heading-title, .entry-date + .post-title, h3.post-title </output>
		<def>
			<size>18px</size>
			<family>Poppins</family>
			<weight>700</weight>
			<align>inherit</align>
			<line_height>24px</line_height>
		</def>
	</font>
	
	
	
